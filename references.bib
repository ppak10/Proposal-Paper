@article{martendal_mitigating_2025,
	title = {Mitigating defects at copper-steel interfaces via a lack of fusion-informed power increment strategy in multi-material laser powder bed fusion},
	volume = {258},
	issn = {0264-1275},
	url = {https://www.sciencedirect.com/science/article/pii/S0264127525010172},
	doi = {10.1016/j.matdes.2025.114597},
	abstract = {Multi-material laser powder bed fusion combines different materials in a single component, enabling enhanced functionalities without additional manufacturing steps. However, joining CuCrZr with 316L is challenging due to property mismatches, often leading to poor interfaces. A novel layer-wise power increment strategy is proposed, starting with low laser power for the initial CuCrZr layer on steel and gradually increasing it for subsequent layers. This approach uses a ring-mode laser to create shallower melt pools, which reduces intermixing but poses a risk of lack of fusion (LoF) defects. A melt pool geometry-based process model using smoothed particle hydrodynamics simulations is developed to predict LoF, optimize power settings, and analyze dynamics not observable experimentally. Experiments show that a saturating exponential power increase reduces LoF up to 24\% compared to a linear strategy. Although minimum power is used at the first interface layer, LoF pores occur between layers 3–8 due to rising thermal conductivity in the growing structure. This effect stabilizes near layer 6, making power control in the first five layers critical. The LoF prediction framework proves effective in developing these parameters. Furthermore, the method confines intermixing to one layer ({\textless}40μm), preventing copper contamination cracking while maintaining robust fusion.},
	urldate = {2025-10-01},
	journal = {Materials \& Design},
	author = {Martendal, C. P. and Esteves, P. D. B. and Lüthi, C. and Deillon, L. and Afrasiabi, M. and Bambach, M.},
	month = oct,
	year = {2025},
	keywords = {Copper contamination cracking, Lack of fusion, Multi-material laser powder bed fusion, Process simulation, Smoothed particle hydrodynamics, Tailored interfaces},
	pages = {114597},
}

@article{chen_microstructures_2022,
	title = {Microstructures and mechanical behaviors of additive manufactured {Inconel} 625 alloys via selective laser melting and laser engineered net shaping},
	volume = {917},
	issn = {0925-8388},
	url = {https://www.sciencedirect.com/science/article/pii/S0925838822019636},
	doi = {10.1016/j.jallcom.2022.165572},
	abstract = {The microstructure and mechanical behaviors of as-built Inconel 625 prepared by selective laser melting (SLM) and laser engineered net shaping (LENS) technologies were investigated in this study. The results show that the microstructure of Inconel 625 alloy prepared by SLM process is composed of obvious columnar and cellular grains. Inconel 625 fabricated by LENS also has columnar and cellular structures, but the grain size is relatively large. Significant anisotropy of mechanical properties is observed in SLM and LENS printed Inconel 625. The mechanical properties in XY direction are higher than those in XZ direction, and the maximum tensile strength of SLM-XY sample is 1241.5 MPa. SLM printed Inconel 625 has a relatively higher dislocation density of 2.8 ± 1.2 × 1014 m−2. The difference in yield strength between SLM and LENS printed Inconel 625 is mainly caused by the different dislocation strengthening effects.},
	urldate = {2025-10-01},
	journal = {Journal of Alloys and Compounds},
	author = {Chen, Fei and Wang, Qin and Zhang, Chi and Huang, Zhifeng and Jia, Mingyong and Shen, Qiang},
	month = oct,
	year = {2022},
	keywords = {Anisotropy, Inconel 625, Laser engineered net shaping, Selective laser melting, Tensile properties},
	pages = {165572},
}

@article{huang_microstructure_2025,
	title = {Microstructure and mechanical properties of stainless steel addictively manufactured via laser powder bed fusion in high-dense process parameter window},
	volume = {928},
	issn = {0921-5093},
	url = {https://www.sciencedirect.com/science/article/pii/S0921509325002515},
	doi = {10.1016/j.msea.2025.148033},
	abstract = {Current additive manufacturing process development primarily focuses on selecting parameters based on density, with performance tuning typically achieved through post-processing. However, research has shown that many alloys possess a relative broad range of high-density process parameters, and fine-tuning these parameters can enable in-situ heat treatment, thereby adjusting the material's structure and properties. Consequently, the practice of selecting a single process parameter based solely on density for various applications remains open to debate. In this paper, commercial 316L stainless steel is employed as a case study to investigate the relative density, microstructure, and mechanical properties of alloys fabricated by laser powder bed fusion (LPBF) within the range of high-density parameters. The work revealed that when the volumetric energy density (VED) exceeds 50.0 J/mm3, SS 316L exhibits a wide range of high-density process parameters, with over 99.5 \% relative density observed in 32 parameters. The mechanical properties were tested using the self-developed high-throughput tensile platform, which shown variations in yield strength, tensile strength, and elongation of 79 MPa, 107.1 MPa, and 26.7 \%, respectively within the high-density parameters range. The microstructure of the high-density parameters, including grain size, morphology, grain boundary angle, crystallographic texture, and geometric dislocation density, exhibited significant distinctions. The process-structure-properties linkage was built and strengthen mechanism was quantitively determined. This research provides a comprehensive analysis the variation in microstructure and mechanical properties across wide range of high-density process parameters. Most importantly, this research demonstrates the potential for precisely tuning process parameters to produce high-performance AM parts with programmable properties.},
	urldate = {2025-10-01},
	journal = {Materials Science and Engineering: A},
	author = {Huang, Guoliang and Chen, Huan and Ma, Zhaodandan and Zhang, Ruiqian and Pei, Jingyuan and Lie, Ziyi and Du, Peinan and Peng, Xiaoqiang and Liu, Ying and Huang, Ke},
	month = apr,
	year = {2025},
	keywords = {316L stainless steel, High-density process parameter, Laser powder bed fusion, Mechanical properties, Microstructure},
	pages = {148033},
}

@article{lu_microstructure_2015,
	title = {Microstructure and {Mechanical} {Properties} of {Long} {Ti}-{6Al}-{4V} {Rods} {Additively} {Manufactured} by {Selective} {Electron} {Beam} {Melting} {Out} of a {Deep} {Powder} {Bed} and the {Effect} of {Subsequent} {Hot} {Isostatic} {Pressing}},
	volume = {46},
	issn = {1543-1940},
	url = {https://doi.org/10.1007/s11661-015-2976-3},
	doi = {10.1007/s11661-015-2976-3},
	abstract = {An array of eight long Ti-6Al-4V rods (diameter: 12 mm; height: 300 mm) have been additively manufactured, vertically and perpendicular to the powder bed, by selective electron beam melting (SEBM). The purpose was to identify and understand the challenges of fabricating Ti-6Al-4V samples or parts from a deep powder bed (more than 200-mm deep) by SEBM and the necessity of applying post heat treatment. The resulting microstructure and mechanical properties of these Ti-6Al-4V rods were characterized along their building (i.e., axial) direction by dividing each rod into three segments (top, middle, and bottom), both before (i.e., as-built) and after hot isostatic pressing (HIP). The as-built microstructure of each rod was inhomogeneous; it was coarsest in the top segment, which showed a near equilibrium α-β lamellar structure, and finest in the bottom segment, which featured a non-equilibrium mixed structure. The tensile properties varied along the rod axis, especially the ductility, but all tensile properties met the requirements specified by ASTM F3001-14. HIP increased the relative density from 99.03 pct of the theoretical density (TD) to 99.90 pct TD and homogenized the microstructure thereby leading to highly consistent tensile properties along the rod axis. The temperature of the stainless steel substrate used in the powder bed was monitored. The as-built inhomogeneous microstructure is attributed to the temperature gradient in the deep powder bed. Post heat treatment is thus necessary for Ti-6Al-4V samples or parts manufactured from a deep powder bed by SEBM. This differs from the additive manufacturing of small samples or parts from a shallow powder bed (less than 100-mm deep) by SEBM.},
	language = {en},
	number = {9},
	urldate = {2025-09-24},
	journal = {Metallurgical and Materials Transactions A},
	author = {Lu, S. L. and Tang, H. P. and Ning, Y. P. and Liu, N. and StJohn, D. H. and Qian, M.},
	month = sep,
	year = {2015},
	keywords = {Additive Manufacturing, Post Heat Treatment, Selective Laser Melting, Stainless Steel Substrate, Tensile Property},
	pages = {3824--3834},
}

@article{jelis_metallurgical_2015,
	title = {Metallurgical and {Mechanical} {Evaluation} of 4340 {Steel} {Produced} by {Direct} {Metal} {Laser} {Sintering}},
	volume = {67},
	issn = {1543-1851},
	url = {https://doi.org/10.1007/s11837-014-1273-8},
	doi = {10.1007/s11837-014-1273-8},
	abstract = {Direct metal laser sintering (DMLS) was used to produce high-strength low-alloy 4340 steel specimens. Mechanical and metallurgical analyses were performed on the specimens to determine the samples with the highest strengths and the least porosity. The optimal process parameters were thus defined based on the corresponding experimental conditions. Additionally, the effects of fabricating specimens with both virgin and recycled powders were studied. Scanning electron microscopy and electron-dispersive spectroscopy were performed on both types of powders to determine the starting morphology and composition. The initial tensile results are promising, suggesting that DMLS can produce specimens equal in strength to wrought materials. However, there is evidence of cracking on several of the heat-treated tensile specimens that is unexplained. Several theories point to disturbances in the build chamber environment that went undetected while the specimens were being fabricated.},
	language = {en},
